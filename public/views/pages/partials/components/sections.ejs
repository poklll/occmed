<% let i = 1; sections.forEach((section)=>{ %>
<div class="section border-radius-all shadow-drop-br">
    <div class="card-body">
        <h4 class="card-title"><%=section.name %></h4>
        <p class="card-text" style="font-family:'Times New Roman', Times, serif; font-size: 25px; color:black;"><%=i*2%></p>
        <div class="progress">
                <div class="progress-bar progress-bar-striped" role="progressbar" style="width: 50%;" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">30%</div>
        </div>
    </div>
</div>
<% i++; }); %>
